// Question 2

#define F_CPU 1000000UL
#include <avr/io.h>
#include <avr/delay.h>


int main()
{
	DDRA= 0b00001111;
	
	while(1)
	{
	
		PORTA = 0b00000001;       
		 _delay_ms(1000);   
		PORTA = 0b00000010;       //turn LED on
		 _delay_ms(1000);
		PORTA = 0b00000100;       //turn LED on
		 _delay_ms(1000);
		PORTA = 0b00001000;       //turn LED on
		 _delay_ms(1000);
	}
		return 0;
}
